  {
    "name": "BlinkNote",
    "version": "1.0",
    "description": "Fastest way to take a note that'll stick around",
    "permissions": ["storage", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "BlinkNote - Alt+B"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+B",
          "mac": "Alt+B"
        },
        "description": "BlinkNote - Alt+B"
      }
    },
    "icons": {
      "16": "images/blinknote-icon16.png",
      "32": "images/blinknote-icon32.png",
      "48": "images/blinknote-icon48.png",
      "128": "images/blinknote-icon128.png"
    }
    "manifest_version": 2
  }